<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
       default-autowire="byName">

    <bean id="sceneDataReader" class="com.taobao.hsf.app.spring.util.HSFSpringConsumerBean"
          init-method="init">
        <property name="interfaceName" value="com.cainiao.training.simuclient.SceneDataReader"/>
        <property name="version" value="${hsf.version.training}"/>
    </bean>

</beans>